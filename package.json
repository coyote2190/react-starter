{
  "name": "react-starter",
  "version": "1.0.0",
  "description": "This template provides a minimal setup to get React working in Vite with HMR and some ESLint rules.",
  "homepage": "https://github.com/coyote2190/react-starter#readme",
  "bugs": {
    "url": "https://github.com/coyote2190/react-starter/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/coyote2190/react-starter.git"
  },
  "license": "ISC",
  "author": "yk",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:check": "tsc --noEmit",
    "preview": "vite preview",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "lint:css": "stylelint 'src/**/*.styles.{ts,tsx}'",
    "lint:css:fix": "stylelint 'src/**/*.styles.{ts,tsx}' --fix",
    "tscheck": "tsc --noEmit",
    "create:component": "ts-node scripts/create-component.ts"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.9.0",
    "@tanstack/react-table": "^8.21.3",
    "i18next": "^25.5.2",
    "i18next-http-backend": "^3.0.2",
    "i18next-icu": "^2.4.0",
    "msw": "^2.11.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-i18next": "^15.7.3",
    "react-redux": "^9.2.0",
    "react-router": "^7.8.2",
    "redux-persist": "^6.0.0",
    "styled-components": "^6.1.19"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.3.1",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "@types/react-redux": "^7.1.34",
    "@vitejs/plugin-react": "^5.0.2",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-sort-destructure-keys": "^2.0.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "globals": "^16.4.0",
    "jsdom": "^26.1.0",
    "postcss-styled-syntax": "^0.7.1",
    "prettier": "^3.6.2",
    "stylelint": "^16.24.0",
    "stylelint-config-recess-order": "^7.3.0",
    "stylelint-config-standard": "^39.0.0",
    "stylelint-order": "^7.0.0",
    "ts-node": "^10.9.2",
    "typescript": "~5.9.2",
    "typescript-eslint": "^8.43.0",
    "unplugin-fonts": "^1.4.0",
    "vite": "^7.1.5",
    "vite-plugin-eslint": "^1.8.1",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  }
